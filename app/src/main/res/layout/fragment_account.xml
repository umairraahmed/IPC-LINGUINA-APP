<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#59C2C2C2"
    tools:context=".account">


    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="44dp"
        android:fontFamily="@font/montserratblack"
        android:text="LINGUINA"
        android:textColor="@color/black"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.075" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="95dp"
        android:layout_height="86dp"
        android:layout_gravity="center"
        android:src="@drawable/linlog2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.272"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.046" />


    <TextView
        android:id="@+id/userwelcomename"
        android:layout_width="162dp"
        android:layout_height="30dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/round_corner"
        android:backgroundTint="#A6A6A4"
        android:gravity="center"
        android:text="User"
        android:textSize="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView16" />


    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/montserrategular"
        android:text="Welcome"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView7" />

    <View
        android:id="@+id/viewacc"
        android:layout_width="363dp"
        android:layout_height="180dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/round_corner"
        android:backgroundTint="#A6A6A4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/userwelcomename"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/accounttap"
        android:layout_width="141dp"
        android:layout_height="141dp"
        android:background="@drawable/round_corner"
        android:fontFamily="@font/montserrategular"
        android:text="Profile"
        android:textAllCaps="false"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/viewacc"
        app:layout_constraintEnd_toStartOf="@+id/Community_Support"
        app:layout_constraintStart_toStartOf="@+id/viewacc"
        app:layout_constraintTop_toTopOf="@+id/viewacc"
        app:layout_constraintVertical_bias="0.538" />

    <Button
        android:id="@+id/Community_Support"
        android:layout_width="141dp"
        android:layout_height="141dp"
        android:background="@drawable/round_corner"
        android:fontFamily="@font/montserrategular"
        android:text="Support"
        android:textAllCaps="false"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/viewacc"
        app:layout_constraintEnd_toEndOf="@+id/viewacc"
        app:layout_constraintHorizontal_bias="0.508"
        app:layout_constraintStart_toEndOf="@+id/accounttap"
        app:layout_constraintTop_toTopOf="@+id/viewacc"
        app:layout_constraintVertical_bias="0.538" />

    <Button
        android:id="@+id/feedback"
        android:layout_width="141dp"
        android:layout_height="141dp"
        android:layout_marginStart="48dp"
        android:background="@drawable/round_corner"
        android:fontFamily="@font/montserrategular"
        android:text="Feedback"
        android:textAllCaps="false"
        android:textColor="@color/black"
        android:textSize="19dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viewacc"
        app:layout_constraintVertical_bias="0.069" />

    <Button
        android:id="@+id/aboutUs"
        android:layout_width="141dp"
        android:layout_height="141dp"
        android:layout_marginStart="28dp"
        android:background="@drawable/round_corner"
        android:fontFamily="@font/montserrategular"
        android:text="About Us"
        android:textAllCaps="false"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/feedback"
        app:layout_constraintTop_toBottomOf="@+id/viewacc"
        app:layout_constraintVertical_bias="0.069" />

    <Button
        android:id="@+id/contactUs"
        android:layout_width="141dp"
        android:layout_height="141dp"
        android:layout_marginStart="132dp"
        android:background="@drawable/round_corner"
        android:fontFamily="@font/montserrategular"
        android:text="Contact Us"
        android:textAllCaps="false"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/aboutUs"
        app:layout_constraintVertical_bias="0.197" />
</androidx.constraintlayout.widget.ConstraintLayout>